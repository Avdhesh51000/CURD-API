<b> Login </b>

<%= form_with model: @users, url: login_path, local: true do |f| %>

<br>
<%= f.label :email %>
<br>
<%= f.text_field :email %>
<br><br>
<%= f.label :password %>
<br>
<%= f.password_field :password %>
<br>
<br>

<%= f.submit "Login" %>

<% end %>

<br>
<br>
<b>Signup Now: &nbsp <%= link_to "click Here" ,new_user_path %></b>
